{
  "expo": {
    "name": "Pustak",
    "slug": "Pustak",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "pustak",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.aayushkumarfabwani.Pustak"
    },
    "web": {
      "output": "static"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/dark_logo_1.png",
          "resizeMode": "cover",
          "backgroundColor": "#131211"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "icon": "./assets/images/dark_logo_1.png",
    "extra": {
      "router": {},
      "eas": {
        "projectId": "93ccb0f1-3e8e-4664-831c-be885f22dcd6"
      }
    }
  }
}
